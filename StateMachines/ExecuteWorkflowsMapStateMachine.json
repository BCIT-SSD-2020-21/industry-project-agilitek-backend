{
  "StartAt": "GetActiveWorkflows",
  "States": {
    "GetActiveWorkflows": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:us-east-1:355997568456:function:getActiveWorkflows",
      "Next": "IterateWorkflows"
    },
    "IterateWorkflows": {
      "Type": "Map",
      "InputPath": "$",
      "ItemsPath": "$.workflows",
      "MaxConcurrency": 0,
      "Iterator": {
        "StartAt": "ExecuteWorkflowQueries",
        "States": {
          "ExecuteWorkflowQueries": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:us-east-1:355997568456:function:executeWorkflowQuery",
            "Next": "TriggerSalesforceFlow"
          },
          "TriggerSalesforceFlow": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:us-east-1:355997568456:function:triggerSalesforceFlow",
            "Next": "Complete"
          },
          "Complete": {
            "Type": "Pass",
            "End": true
          }
        }
      },
      "ResultPath": "$",
      "End": true
    }
  }
}